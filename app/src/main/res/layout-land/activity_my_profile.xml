<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/background_my_profile"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_my_profile_land_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_my_profile_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_my_profile_land_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_my_profile_land_end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />


    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/dark_blue"
        app:layout_constraintBottom_toBottomOf="@id/guideline_my_profile_land_main"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

    </View>

    <TextView
        android:id="@+id/setting_my_profile_tv_land"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/audio_wide"
        android:text="@string/tv_my_profile_settings"
        android:textColor="@color/white"
        android:textSize="@dimen/tv_settings_my_profile"
        app:layout_constraintStart_toStartOf="@+id/guideline_my_profile_land_start"
        app:layout_constraintTop_toTopOf="@+id/guideline_my_profile_top" />


    <ImageView
        android:id="@+id/my_profile_icon_iv_land"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/iv_my_profile_person"
        android:src="@mipmap/me_round"
        app:layout_constraintBottom_toBottomOf="@+id/guideline_my_profile_land_main"
        app:layout_constraintEnd_toStartOf="@+id/guideline_my_profile_land_start_first"
        app:layout_constraintTop_toTopOf="@+id/guideline_my_profile_top" />

    <TextView
        android:id="@+id/tv_my_profile_user_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/audio_wide"
        android:text="@string/tv_user_name"
        android:textColor="@color/white"
        android:textSize="@dimen/tv_user_name"
        app:layout_constraintBottom_toTopOf="@+id/tv_my_profile_land_career"
        app:layout_constraintStart_toStartOf="@id/guideline_my_profile_land_start_second"
        app:layout_constraintTop_toTopOf="@+id/guideline_my_profile_top"
        app:layout_constraintVertical_chainStyle="packed" />


    <TextView
        android:id="@+id/tv_my_profile_land_career"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/audio_wide"
        android:text="@string/tv_profession"
        android:textColor="@color/semi_transparent_blue_gray"
        android:textSize="@dimen/tv_secondary_my_profile"
        app:layout_constraintBottom_toTopOf="@id/tv_my_profile_land_address"
        app:layout_constraintEnd_toEndOf="@id/tv_my_profile_user_name"
        app:layout_constraintStart_toStartOf="@id/tv_my_profile_user_name"
        app:layout_constraintTop_toBottomOf="@id/tv_my_profile_user_name"

        />

    <TextView
        android:id="@+id/tv_my_profile_land_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/audio_wide"
        android:text="@string/tv_address"
        android:textColor="@color/semi_transparent_blue_gray"
        android:textSize="@dimen/tv_secondary_my_profile"
        app:layout_constraintBottom_toTopOf="@id/guideline_my_profile_land_main"
        app:layout_constraintEnd_toEndOf="@id/tv_my_profile_user_name"
        app:layout_constraintStart_toStartOf="@id/tv_my_profile_user_name"
        app:layout_constraintTop_toBottomOf="@id/tv_my_profile_land_career" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_my_profile_land_first"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.56" />

    <ImageView
        android:id="@+id/iv_my_profile_land_facebook_icon"
        style="@style/facebook_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/iv_link_to_facebook"
        app:layout_constraintEnd_toStartOf="@id/guideline_my_profile_land_start_first"
        app:layout_constraintTop_toBottomOf="@id/guideline_my_profile_land_first" />

    <ImageView
        android:id="@+id/iv_my_profile_linkedin_land_icon"
        style="@style/linkedin_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/iv_link_to_linkedin"
        app:layout_constraintEnd_toStartOf="@id/guideline_my_profile_land_start_second"
        app:layout_constraintStart_toEndOf="@id/guideline_my_profile_land_start_first"
        app:layout_constraintTop_toBottomOf="@id/guideline_my_profile_land_first" />

    <ImageView
        android:id="@+id/iv_my_profile_land_instagram_icon"
        style="@style/instagram_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/iv_link_to_instagram"
        app:layout_constraintStart_toEndOf="@id/guideline_my_profile_land_start_second"
        app:layout_constraintTop_toBottomOf="@id/guideline_my_profile_land_first" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_my_profile_land_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.8" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_my_profile_land_start_first"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.4" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bt_my_profile_land_edit"
        style="@style/edit_bt_background"
        android:layout_width="0dp"
        android:layout_height="wrap_content"

        android:text="@string/bt_my_profile_edit_profile"
        app:layout_constraintEnd_toStartOf="@id/guideline_my_profile_land_start_first"
        app:layout_constraintStart_toStartOf="@id/guideline_my_profile_land_start"
        app:layout_constraintTop_toBottomOf="@id/guideline_my_profile_land_second" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_my_profile_land_start_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.6" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bt_my_profile_view_my_contacts"
        style="@style/view_my_contacts_bt_background"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/bt_my_profile_view_my_contacts"
        app:layout_constraintEnd_toEndOf="@id/guideline_my_profile_land_end"
        app:layout_constraintStart_toEndOf="@id/guideline_my_profile_land_start_second"
        app:layout_constraintTop_toBottomOf="@id/guideline_my_profile_land_second" />


</androidx.constraintlayout.widget.ConstraintLayout>